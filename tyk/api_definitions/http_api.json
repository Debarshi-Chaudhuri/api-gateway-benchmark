{
  "name": "HTTP API",
  "slug": "http-api",
  "api_id": "http-api-1",
  "org_id": "default",
  "use_keyless": true,
  "proxy": {
    "listen_path": "/http-api/",
    "target_url": "http://http-service:8000/",
    "strip_listen_path": true
  },
  "active": true,
  "disable_rate_limit": false,
  

  "global_rate_limit": {
    "rate": 20,
    "per": 1
  },
  

  "extended_paths": {
    "ignored": [],
    "white_list": [],
    "black_list": [],
    "rate_limit": [
      {
        "path": "/api/data",
        "method": "GET",
        "rate": 20,
        "per": 1,
        "throttle_interval": 0,
        "throttle_retry_limit": 0,
        "max_query_depth": 0,
        "throttle_policy": "",
        "throttle_data": {},
        "enable_max_burst": true,
        "max_burst_multiplier": 2.5
      }
    ]
  }
}