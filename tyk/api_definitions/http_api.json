{
  "name": "HTTP API",
  "slug": "http-api",
  "api_id": "http-api-1",
  "org_id": "default",
  "use_keyless": true,
  "definition": {
    "location": "header",
    "key": "version"
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true
      }
    }
  },
  "proxy": {
    "listen_path": "/http-api/",
    "target_url": "http://http-service:8000/",
    "strip_listen_path": true
  },
  "active": true,
  "cache_options": {
    "cache_timeout": 60,
    "enable_cache": true,
    "cache_all_safe_requests": false,
    "cache_response_codes": [200]
  },
  "global_rate_limit": {
    "rate": 100,
    "per": 60
  },
  "uptime_tests": {
    "check_list": [
      {
        "url": "http://http-service:8000/health",
        "method": "GET",
        "headers": {},
        "body": "",
        "timeout": 10,
        "check_interval": 10
      }
    ],
    "config": {
      "expire_utime_after": 300,
      "service_discovery": {
        "use_discovery_service": false
      },
      "recheck_wait": 30
    }
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "expires": "",
        "paths": {
          "ignored": [],
          "white_list": [],
          "black_list": []
        },
        "use_extended_paths": true,
        "extended_paths": {
          "ignore": [
            {
              "path": "/health",
              "method_actions": {
                "GET": {
                  "action": "no_action",
                  "code": 200,
                  "data": "",
                  "headers": {}
                }
              }
            }
          ]
        }
      }
    }
  }
}