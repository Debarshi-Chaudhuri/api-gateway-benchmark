{
  "url_pattern": "/service.DataService/GetData",
  "encoding": "json",
  "method": "POST",
  "host": ["http://krakend-grpc-gateway:8090"],
  "disable_host_sanitize": false,
  "extra_config": {
    "qos/circuit-breaker": {
      "interval": 60,
      "timeout": 10,
      "max_errors": 3,
      "log_level": "error"
    },
    "qos/ratelimit/proxy": {
      "max_rate": 50,
      "capacity": 1
    }
  }
}